#include<iostream>
#include<iomanip>
using namespace std;
int **matr(int n, int m, int q, int A[], int B[])
{
	int  i, j, u;
	int **Matryca1 = new int *[m];
	for (i = 0; i < m; i++)
		Matryca1[i] = new int[n];
	for (i = 0; i < m; i++)
	{
		for (j = 0; j < n; j++)
		{
			for (u = 0; u < n*m; u++)
			{
				Matryca1[i][j] = A[u];
			}
		}
	}
	int **Matryca2 = new int *[n];
	for (i = 0; i < n; i++)
		Matryca2[i] = new int[q];
	for (i = 0; i < n; i++)
	{
		for (j = 0; j < q; j++)
		{
			for (u = 0; u < n*q; u++)
			{
				Matryca2[i][j] = B[u];
			}
		}
	}
	int** result;
	result = new int*[m];
	for (int i = 0; i < m; i++)
	{
		result[i] = new int[q];

	}
	int **Matryca3 = new int *[i];
	for (i = 0; i < m; i++)
		Matryca3[i] = new int[j];
	for (i = 0; i < m; i++)
	{
		for (j = 0; j < q; j++)
		{
			Matryca3[i][j] = 0;
			for (int k = 0; k < q; k++)
				Matryca3[i][j] += (Matryca1[i][k] * Matryca2[k][j]);
		}
		result[i][j] = **Matryca3;
		;
	}
return result;
}
int main()
{
	return 0;
}
